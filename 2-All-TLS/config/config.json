{
  "log": {
      "access": "/var/log/xray/access.log",
      "error": "/var/log/xray/error.log",
      "loglevel": "warning"
  },
  "inbounds": [
      {
          "port": 443,
          "protocol": "vless",
          "settings": {
              "clients": [
                  {
                      "id": "***UUID***",
                      "flow": "xtls-rprx-direct",
                      "level": 0,
                      "email": "vless@ganji.dev"
                  }
              ],
              "decryption": "none",
              "fallbacks": [
                  {
                      "dest": 1310,
                      "xver": 1
                  },
                  {
                      "path": "/chat",
                      "dest": 1234,
                      "xver": 1
                  },
                  {
                      "path": "/about",
                      "dest": 2345,
                      "xver": 1
                  },
                  {
                      "path": "/graphql",
                      "dest": 3456,
                      "xver": 1
                  }
              ]
          },
          "streamSettings": {
              "network": "tcp",
              "security": "xtls",
              "xtlsSettings": {
                  "alpn": [
                      "http/1.1"
                  ],
                  "certificates": [
                      {
                          "certificateFile": "/ssl/cert.pem",
                          "keyFile": "/ssl/key.pem"
                      }
                  ]
              }
          }
      },
      {
          "port": 1310,
          "listen": "127.0.0.1",
          "protocol": "trojan",
          "settings": {
              "clients": [
                  {
                      "password": "***Password***",
                      "level": 0,
                      "email": "trojan@ganji.dev"
                  }
              ],
              "fallbacks": [
                  {
                      "dest": 8080
                  }
              ]
          },
          "streamSettings": {
              "network": "tcp",
              "security": "none",
              "tcpSettings": {
                  "acceptProxyProtocol": true
              }
          }
      },
      {
          "port": 1234,
          "listen": "127.0.0.1",
          "protocol": "vless",
          "settings": {
              "clients": [
                  {
                      "id": "***UUID***",
                      "level": 0,
                      "email": "ws@ganji.dev"
                  }
              ],
              "decryption": "none"
          },
          "streamSettings": {
              "network": "ws",
              "security": "none",
              "wsSettings": {
                  "acceptProxyProtocol": true,
                  "path": "/chat"
              }
          }
      },
      {
          "port": 2345,
          "listen": "127.0.0.1",
          "protocol": "vmess",
          "settings": {
              "clients": [
                  {
                      "id": "***UUID***",
                      "level": 0,
                      "email": "vmess@ganji.dev"
                  }
              ]
          },
          "streamSettings": {
              "network": "tcp",
              "security": "none",
              "tcpSettings": {
                  "acceptProxyProtocol": true,
                  "header": {
                      "type": "http",
                      "request": {
                          "path": [
                              "/about"
                          ]
                      }
                  }
              }
          }
      },
      {
          "port": 3456,
          "listen": "127.0.0.1",
          "protocol": "vmess",
          "settings": {
              "clients": [
                  {
                      "id": "***UUID***",
                      "level": 0,
                      "email": "vmessws@ganji.dev"
                  }
              ]
          },
          "streamSettings": {
              "network": "ws",
              "security": "none",
              "wsSettings": {
                  "acceptProxyProtocol": true,
                  "path": "/graphql"
              }
          }
      }
  ],
  "outbounds": [
      {
          "protocol": "freedom"
      }
  ],
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  }
}
